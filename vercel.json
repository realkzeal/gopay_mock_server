{
    "version": 2,
    "functions": {
      "api/index.js": {
        "maxDuration": 30
      }
    },
    "rewrites": [
      {
        "source": "/mock_server/(.*)",
        "destination": "/api/index.js"
      },
      {
        "source": "/api/(.*)",
        "destination": "/api/index.js"
      },
      {
        "source": "/(.*)",
        "destination": "/api/index.js"
      }
    ]
  }